<h1>Listando Artículos</h1>

<%= link_to 'Artículo Nuevo', new_article_path %> 
<table>
    <tr>
        <th>Titulo</th>
        <th>Texto</th>
        <th colspan="2"></th>
    </tr>
 
    <% @articles.each do |article| %>
        <tr>
            <td><%= article.titulo %></td>
            <td><%= article.texto %></td>
            <td><%= link_to 'Show', article_path(article) %></td>
            <td><%= link_to 'Edit', edit_article_path(article) %></td>
        </tr>
    <% end %>
</table>